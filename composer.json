{
    "name": "paulhenri-l/laravel-pub-sub-engine",
    "description": "laravel-pub-sub-engine",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "paulhenri-l",
            "email": "25308170+paulhenri-l@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0",
        "paulhenri-l/laravel-engine-core": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.1",
        "paulhenri-l/php-cs-config": "^1.0",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "PaulhenriL\\LaravelPubSubEngine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PaulhenriL\\LaravelPubSubEngine\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PaulhenriL\\LaravelPubSubEngine\\LaravelPubSubEngineServiceProvider"
            ],
            "aliases": {}
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    }
}
